<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=1280,height=1024, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no" />
		<title>用户登录</title>
		<link rel="stylesheet" href="login.css" />
		<script type="text/javascript" src="../../script/jquery-1.8.3.js" ></script>
		<script type="text/javascript">
			$(function(){
				$(".loginDialog >div >div>button").click(function(){
					var name = $(this).attr("name");
					
					if(name == "login"){
						parentWindowEx.execute("onLogin({username:'youxz',cnname:'尤小忠'})");
						rexseeBrowser.dismiss();
						return;
					}
					
					if(name == "cancel"){
						rexseeBrowser.dismiss();
					}
					
				});
			});
		</script>
	</head>
	<body scroll="no">
	<div class="loginDialog">
		
		<div>
			<ul>
				<li><label>用户名:</label><input type="text" name="username" value="admin" />
				<li><label>密&nbsp;&nbsp;&nbsp;&nbsp;码:</label><input type="password" name="password" value="123456" />
			</ul>
			<div>
				<button name="login">登 录</button> <button name="cancel">取 消</button>	
			</div>
		</div>
		
		
	</div>
	</body>
</html>
